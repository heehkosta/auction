      function get_main_image_callback()
      {
      if(xmlHttp.readyState == 4) {
      if(xmlHttp.status == 200) {
      if (typeof(document.all.MainPicDiv) == "object") 
      {
      document.all.MainPicDiv.innerHTML = xmlHttp.responseText;
      }
      }
      }
