      function get_main_image(seq)
      {
      createXMLHttpRequest();
      xmlHttp.onreadystatechange = get_main_image_callback;
      xmlHttp.open("GET", "get_main_image.asp?seq="+escape(seq), true);
      xmlHttp.send(null);
